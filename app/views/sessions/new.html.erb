

                            

  
<h1>Connexion</h1>
<input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
<%= form_with(url: sessions_path, method: "post") do |form| %>
  <%= form.label :email %>
  <%= form.email_field :email %>

  <%= form.label :password %>
  <%= form.password_field :password %>

  <%= form.submit "Se connecter" %>
<% end %>         </br></br>
                        